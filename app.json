{
  "expo": {
    "name": "Sales Diary",
    "slug": "sales-diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access to scan barcodes for adding items.",
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to select product images."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8a16411-e2be-4069-9e91-26ab119f07ea"
      }
    },
    "android": {
      "package": "com.ybill"
    }
  }
}
